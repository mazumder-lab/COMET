{
    "nb_epochs": 1000,
    "train_batch_size": {
        "optuna_type": "suggest_categorical",
        "values": [64]
    },
    "val_batch_size": 128,
    "grad_acc_steps": 1,
    "taskset_id": 15,
    "optimizer": {
        "type": "Adam",
        "params": {
            "learning_rate": {
                "optuna_type": "suggest_loguniform",
                "values": [1e-4, 5e-3]
            }
        }
    },
    "callbacks": [
        {
            "type": "TerminateOnNaN",
            "params": {}
        },
        {
            "type": "EarlyStopping",
            "params": {
                "patience": 50,
                "restore_best_weights": true
            }
        }
    ]
}