{
    "nb_epochs": 1,
    "train_batch_size": {
        "optuna_type": "suggest_categorical",
        "values": [128, 256, 512, 1024]
    },
    "val_batch_size": 1024,
    "grad_acc_steps": 1,
    "taskset_id": 8,
    "optimizer": {
        "type": "SGD",
        "params": {
            "learning_rate": {
                "optuna_type": "suggest_categorical",
                "values": [1e-2, 5e-2, 7.5e-2, 1e-1, 2.5e-1, 5e-1, 7.5e-1, 1.0]
            }        
        }
    },
    "callbacks": [
        {
            "type": "EarlyStopping",
            "params": {
                "monitor": "val_loss",
                "patience": 20,
                "restore_best_weights": true
            }
        },
        {
            "type": "TerminateOnNaN",
            "params": {}
        }
    ],
    "enforce_simplex_tol": 1e-2,
    "display_task_metrics_each_epoch": false
}