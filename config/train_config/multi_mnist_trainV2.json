{
    "nb_epochs": 500,
    "train_batch_size": {
        "optuna_type": "suggest_categorical",
        "values": [128,256,512,1024]
    },
    "val_batch_size": 128,
    "grad_acc_steps": 1,
    "taskset_id": 9,
    "task_weights": [0.5, 0.5],
    "optimizer": {
        "type": "Adam",
        "params": {
            "learning_rate": {
                "optuna_type": "suggest_categorical",
                "values": [1e-4, 5e-4, 1e-3, 5e-3, 1e-2,  5e-2, 1e-1]
            }
        }
    },
    "callbacks": [
        {
            "type": "TerminateOnNaN",
            "params": {}
        },
        {
            "type": "EarlyStopping",
            "params": {
                "patience": 25,
                "restore_best_weights": true
            }
        }
    ]
}