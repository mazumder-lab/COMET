{
    "nb_epochs": 1,
    "train_batch_size": {
        "optuna_type": "suggest_categorical",
        "values": [128, 256, 512, 1024]
    },
    "val_batch_size": 1024,
    "grad_acc_steps": 1,
    "taskset_id": 4,
    "task_weights": [0.9, 0.1],
    "optimizer": {
        "type": "SGD",
        "params": {
            "learning_rate": {
                "optuna_type": "suggest_categorical",
                "values": [1e-3, 1e-2, 1e-1, 5e-1, 1]
            }
        }
    },
    "callbacks": [
        {
            "type": "TerminateOnNaN",
            "params": {}
        },
        {
            "type": "EarlyStopping",
            "params": {
                "patience": 15,
                "restore_best_weights": true
            }
        }
    ]
}