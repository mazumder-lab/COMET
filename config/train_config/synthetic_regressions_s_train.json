{
    "nb_epochs": 500,
    "train_batch_size": {
        "optuna_type": "suggest_categorical",
        "values": [128, 256, 512, 1024]
    },
    "val_batch_size": 2048,
    "grad_acc_steps": 1,
    "taskset_id": 3,
    "task_weights": [0.25,0.25,0.25,0.25],
    "optimizer": {
        "type": "SGD",
        "params": {
            "learning_rate": {
                "optuna_type": "suggest_categorical",
                "values": [1e-3, 1e-2, 5e-2, 7.5e-2, 1e-1, 0.25, 5e-1, 1.0]
            }
        }
    },
    "callbacks": [
        {
            "type": "TerminateOnNaN",
            "params": {}
        },
        {
            "type": "EarlyStopping",
            "params": {
                "patience": 20,
                "restore_best_weights": true
            }
        }
    ]
}